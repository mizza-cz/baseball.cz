<section class="matches-section">
  <div class="matchItem">
    <div class="text-md fw-bold matchItem__date">
      pá 12. 4. 2025
      <div class="">19:00</div>
    </div>
    <div class="d-flex gap-20 align-items-center matchItem__teams">
      <div class="d-flex gap-12 align-items-center">
        <img
          src="images/content/club-1.png"
          width="48"
          height="48"
          class="object-fit-contain"
          alt="club"
        />
        <img
          src="images/content/club-2.png"
          width="48"
          height="48"
          class="object-fit-contain"
          alt="club"
        />
      </div>
      <div class="h4 fw-bold text-primary d-flex align-items-center gap-2">
        Eagles Praha
        <span class="text-tertiary">×</span>
        Kotlářka Praha
      </div>
    </div>
    <div class="matchItem__content">
      <a href="#" class="text-primary fw-bold linkDecoration matchItem__results"
        >11:13</a
      >
      <div
        class="d-flex align-items-center gap-2 text-md text-tertiary matchItem__location"
      >
        <img
          src="images/ico/marker.svg"
          width="20 "
          height="20"
          class="object-fit-contain"
          alt="marker"
        />
        Hřiště Krč, Praha
      </div>
      <ul class="matchItem__links text-md">
        <li>
          <a href="#" target="_blank">
            <img
              src="images/content/ct.png"
              width="71"
              height="16"
              class="object-fit-contain"
              alt="ct-sport"
            />
          </a>
        </li>
        <li>
          <a href="#" class="matchItem__link text-secondary">
            {% include "@images/play-live.svg" %} TV
          </a>
        </li>
        <li>
          <a href="#" class="matchItem__link text-primary">
            {% include "@images/line.svg" %} PBP
          </a>
        </li>
      </ul>
    </div>
  </div>
